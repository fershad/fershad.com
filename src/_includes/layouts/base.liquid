    {% include "partials/metadata" %}
    <link rel="stylesheet" href="/css/index.css">
    {% if css %}
      <link rel="stylesheet" href="/css/{{ css }}">
    {% endif %}
  </head>

  <body>
    {% include 'partials/header.liquid' %}
